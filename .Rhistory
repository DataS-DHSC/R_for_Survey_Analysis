# Let's keep only adults in for this
adults <- nhanes %>% dplyr::filter(Age >= 18)
# plot scatter plot
plot(x = adults$Height, y = adults$Weight)
# 2.1 Transparency
ggplot(data = adults, aes(x = Height, y = Weight)) +
geom_point(alpha = 0.4)
